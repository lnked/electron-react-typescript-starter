{
  "name": "electron-react-typescript-starter",
  "version": "1.0.0",
  "main": "dist/main.js",
  "author": "Edik Bulikyan <ed.proff@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build-main": "cross-env NODE_ENV=production babel src/app/main.dev.js -o dist/main.js",
    "prestart": "yarn build-main",
    "start": "electron dist/main.js"
  },
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.4",
    "@babel/plugin-proposal-class-properties": "7.5.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.5.4",
    "@types/jest": "24.0.15",
    "@types/node": "12.6.4",
    "@types/semver": "6.0.1",
    "@types/webpack": "4.4.35",
    "concurrently": "4.1.1",
    "cross-env": "5.2.0",
    "electron": "5.0.7",
    "electron-devtools-installer": "2.2.4",
    "husky": "3.0.0",
    "jest": "24.8.0",
    "lint-staged": "9.2.0",
    "memory-fs": "0.4.1",
    "rimraf": "2.6.3",
    "tslint": "5.18.0",
    "typescript": "3.5.3",
    "webpack": "4.35.3",
    "webpack-cli": "3.3.6"
  },
  "dependencies": {
    "core-js": "3.1.4",
    "electron-is-dev": "1.1.0",
    "node-ipc": "9.1.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-hot-loader": "4.12.7",
    "react-router-dom": "5.0.1",
    "uuid": "3.3.2"
  },
  "build": {
    "productName": "ElectronReact",
    "appId": "org.develar.ElectronReact",
    "files": [
      "app/dist/",
      "app/app.html",
      "app/main.prod.js",
      "app/main.prod.js.map",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "publish": {
      "provider": "github",
      "owner": "electron-react-boilerplate",
      "repo": "electron-react-boilerplate",
      "private": false
    }
  }
}
