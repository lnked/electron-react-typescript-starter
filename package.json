{
  "name": "electron-react-typescript-starter",
  "version": "1.0.1",
  "main": "dist/main.js",
  "author": "Edik Bulikyan <ed.proff@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clear:dist": "rimraf -rf ./dist/*",
    "clear:cache": "rimraf -rf ./.cache/*",
    "build-main": "cross-env NODE_ENV=production babel src/app/main.dev.js -o dist/main.js --minified --no-comments",
    "build-main:dev": "cross-env NODE_ENV=production babel src/app/main.dev.js -o dist/main.js",
    "prestart": "yarn build-main",
    "prebuild": "yarn clear:dist && cross-env NODE_ENV=production yarn lint",
    "start": "cross-env NODE_ENV=development electron dist/main.js",
    "build": "env NODE_ENV=production webpack --config configs/webpack --env production",
    "build:display": "env NODE_ENV=production webpack --display-modules --config configs/webpack --env production",
    "serve": "webpack-dev-server --config configs/webpack --env development",
    "analyze": "yarn clear:cache && yarn build --analyze",
    "release": "yarn build --release",
    "profile": "webpack --config configs/webpack --env production --profile --json > stats.json",
    "snapshots": "node configs/snapshots",
    "lint": "tslint -t stylish -p tsconfig.json 'src/**/*.ts?(x)'",
    "lint:fix": "yarn lint --fix",
    "typecheck": "tsc",
    "pretest": "cross-env NODE_ENV=test yarn lint",
    "test": "jest",
    "test:watch": "yarn test -- --watch",
    "ci": "yarn test & yarn typecheck & yarn lint",
    "add-component": "node configs/utils/add --essence component",
    "add-page": "node configs/utils/add --essence page",
    "add-layout": "node configs/utils/add --essence layout",
    "add-fragment": "node configs/utils/add --essence fragment"
  },
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.4",
    "@babel/plugin-proposal-class-properties": "7.5.0",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-proposal-export-default-from": "7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "7.5.2",
    "@babel/plugin-proposal-object-rest-spread": "7.5.4",
    "@babel/plugin-syntax-async-generators": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-arrow-functions": "7.2.0",
    "@babel/plugin-transform-react-constant-elements": "7.5.0",
    "@babel/plugin-transform-react-inline-elements": "7.2.0",
    "@babel/plugin-transform-react-jsx": "7.3.0",
    "@babel/plugin-transform-runtime": "7.5.0",
    "@babel/plugin-transform-spread": "7.2.2",
    "@babel/plugin-transform-template-literals": "7.4.4",
    "@babel/preset-env": "7.5.4",
    "@gfx/zopfli": "1.0.14",
    "@types/jest": "24.0.15",
    "@types/node": "12.6.6",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-hot-loader": "4.1.0",
    "@types/react-router-dom": "4.3.4",
    "@types/semver": "6.0.1",
    "@types/webpack": "4.4.35",
    "babel-loader": "8.0.6",
    "cache-loader": "4.0.1",
    "compression-webpack-plugin": "3.0.0",
    "concurrently": "4.1.1",
    "copy-webpack-plugin": "5.0.3",
    "cross-env": "5.2.0",
    "css-loader": "3.0.0",
    "dotenv": "8.0.0",
    "electron": "5.0.7",
    "electron-devtools-installer": "2.2.4",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "file-loader": "4.0.0",
    "fs": "0.0.1-security",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.0",
    "image-webpack-loader": "5.0.0",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-webpack-plugin": "2.4.2",
    "inspectpack": "4.2.1",
    "jest": "24.8.0",
    "jest-emotion": "10.0.14",
    "lint-staged": "9.2.0",
    "markdown-loader": "5.0.0",
    "memory-fs": "0.4.1",
    "path": "0.12.7",
    "preload-webpack-plugin": "2.3.0",
    "raw-loader": "3.0.0",
    "rimraf": "2.6.3",
    "script-ext-html-webpack-plugin": "2.1.4",
    "source-map-loader": "0.2.4",
    "style-loader": "0.23.1",
    "svg-url-loader": "3.0.0",
    "terser-webpack-plugin": "1.3.0",
    "thread-loader": "2.1.2",
    "tslint": "5.18.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-react": "4.0.0",
    "tslint-react-hooks": "2.2.1",
    "typescript": "3.5.3",
    "url-loader": "2.0.1",
    "webpack": "4.36.1",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-chunk-hash": "0.6.0",
    "webpack-cleanup-plugin": "0.5.1",
    "webpack-cli": "3.3.6",
    "webpack-dev-server": "3.7.2",
    "webpack-merge": "4.2.1",
    "workbox-webpack-plugin": "4.3.1"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "7.5.4",
    "core-js": "3",
    "electron-is-dev": "1.1.0",
    "node-ipc": "9.1.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-hot-loader": "4.12.7",
    "react-router-dom": "5.0.1",
    "uuid": "3.3.2"
  },
  "build": {
    "productName": "ElectronReact",
    "appId": "org.develar.ElectronReact",
    "files": [
      "app/dist/",
      "app/app.html",
      "app/main.prod.js",
      "app/main.prod.js.map",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "publish": {
      "provider": "github",
      "owner": "electron-react-boilerplate",
      "repo": "electron-react-boilerplate",
      "private": false
    }
  }
}
