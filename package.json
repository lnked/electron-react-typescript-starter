{
  "name": "electron-react-typescript-starter",
  "version": "1.0.1",
  "main": "dist/main.js",
  "author": "Edik Bulikyan <ed.proff@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clear:dist": "rimraf -rf ./dist/*",
    "clear:cache": "rimraf -rf ./.cache/*",
    "preload": "rollup src/app/preload.js -c --file dist/preload.js",
    "prestart": "cross-env NODE_ENV=development yarn clear:dist && yarn preload && yarn main:build",
    "prebuild": "cross-env NODE_ENV=production yarn clear:dist && yarn main:build && yarn lint",
    "electron": "electron dist/main.js",
    "main:build": "rollup src/app/main/index.js -c --file dist/main.js",
    "main:start": "yarn main:build -w",
    "react:start": "webpack-dev-server --config configs/webpack --env development",
    "react:build": "webpack --config configs/webpack --env production",
    "analyze": "yarn clear:cache && yarn build --analyze",
    "start": "concurrently -p -c -k -r 'yarn:electron' 'yarn:react:start'",
    "snapshots": "node configs/snapshots",
    "lint": "tslint -t stylish -p tsconfig.json 'src/**/*.ts?(x)'",
    "lint:fix": "yarn lint --fix",
    "typecheck": "tsc",
    "pretest": "cross-env NODE_ENV=test yarn lint",
    "test": "jest",
    "release": "electron-builder --dir",
    "test:watch": "yarn test -- --watch",
    "ci": "yarn test & yarn typecheck & yarn lint",
    "add-component": "node configs/utils/add --essence component",
    "add-page": "node configs/utils/add --essence page",
    "add-layout": "node configs/utils/add --essence layout",
    "add-fragment": "node configs/utils/add --essence fragment"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-proposal-export-default-from": "7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "7.5.2",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/plugin-syntax-async-generators": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-arrow-functions": "7.2.0",
    "@babel/plugin-transform-async-to-generator": "7.5.0",
    "@babel/plugin-transform-react-constant-elements": "7.5.0",
    "@babel/plugin-transform-react-inline-elements": "7.2.0",
    "@babel/plugin-transform-react-jsx": "7.3.0",
    "@babel/plugin-transform-runtime": "7.5.5",
    "@babel/plugin-transform-spread": "7.2.2",
    "@babel/plugin-transform-template-literals": "7.4.4",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@emotion/babel-preset-css-prop": "10.0.14",
    "@gfx/zopfli": "1.0.14",
    "@types/jest": "24.0.15",
    "@types/node": "12.6.8",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-hot-loader": "4.1.0",
    "@types/react-router-dom": "4.3.4",
    "@types/semver": "6.0.1",
    "@types/webpack": "4.32.0",
    "babel-loader": "8.0.6",
    "cache-loader": "4.1.0",
    "cleanup-webpack-plugin": "0.0.1",
    "compression-webpack-plugin": "3.0.0",
    "concurrently": "4.1.1",
    "copy-webpack-plugin": "5.0.3",
    "cross-env": "5.2.0",
    "css-loader": "3.1.0",
    "dotenv": "8.0.0",
    "electron": "5.0.7",
    "electron-builder": "21.1.1",
    "electron-devtools-installer": "2.2.4",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "file-loader": "4.1.0",
    "fs": "0.0.1-security",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.1",
    "image-webpack-loader": "5.0.0",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-webpack-plugin": "2.4.2",
    "inquirer": "6.5.0",
    "inspectpack": "4.2.1",
    "jest": "24.8.0",
    "jest-emotion": "10.0.14",
    "lint-staged": "9.2.0",
    "markdown-loader": "5.0.0",
    "memory-fs": "0.4.1",
    "path": "0.12.7",
    "preload-webpack-plugin": "2.3.0",
    "prettier": "1.18.2",
    "raw-loader": "3.1.0",
